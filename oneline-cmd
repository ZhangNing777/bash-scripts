### install/start/stop mysql
brew search mysql
brew install mysql@5.7
brew uninstall mysql@5.7
brew services start mysql@5.7
brew services stop mysql@5.7

### ps and grep
ps aux | grep mysql

### kill pid
kill -9 pid

### git cmd
git status
git diff

git add oneline-cmd
git status
git diff --cached

git commit -m "add git/kill/ps cmd."
git status
git log
git push

git log
git show commitid
